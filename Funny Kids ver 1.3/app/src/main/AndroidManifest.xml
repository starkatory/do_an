<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.package_for_music.ListMusicActivity" />
        <activity android:name="com.package_for_video.ListVideoActivity"></activity>
        <activity android:name="com.package_for_learning.Learnning_Activity"></activity>
        <activity android:name="com.package_for_story.ListStoryActivity"></activity>

    </application>

</manifest>
